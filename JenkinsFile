  agent any
  
  stages {
  
    stage("build") {
   
      steps {
        echo 'Building Jack's React Application...'
      }
    
    }
    
    stage("test") {
   
      steps {
         echo 'Running some tests...'
      }
    
    }
    
    stage("deploy") {
   
      steps {
        echo 'Deploying this'
      }
    
    }
    
  }
  
}
